# Sentry Configuration
SENTRY_DSN=your_sentry_dsn_here
SENTRY_ENVIRONMENT=production

# Optional: Enable Sentry in development (disabled by default)
# SENTRY_ENABLE_DEV=true

# Crash Reporter Configuration
# For Sentry: use your Sentry Minidump endpoint
# Format: https://o[org-id].ingest.sentry.io/api/[project-id]/minidump/?sentry_key=[public-key]
CRASH_REPORT_URL=your_crash_report_url_here

# Optional: Enable crash reporter in development (disabled by default)
# CRASH_REPORTER_ENABLE_DEV=true

# Application Configuration
NODE_ENV=production
